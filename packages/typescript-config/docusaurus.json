{
  "$schema": "https://json.schemastore.org/tsconfig",
  "display": "Docusaurus",
  "extends": "./base.json",
  "compilerOptions": {
    "allowJs": true,
    "jsx": "preserve",
    "moduleResolution": "bundler",
    "module": "esnext",
    "noEmit": true,
    "types": [
      "node",
      "@docusaurus/module-type-aliases",
      "@docusaurus/theme-classic"
    ],
    "baseUrl": ".",
    "paths": {
      "@site/*": ["./*"]
    },
  }
}